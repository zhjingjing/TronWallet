<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="titleText" format="string" />
        <attr name="titleColor" format="color" />
        <attr name="titleSize" format="dimension" />

        <attr name="leftBtnText" format="string" />
        <attr name="leftBtnTextColor" format="color" />
        <attr name="leftBtnTextSize" format="dimension" />
        <attr name="leftBtnIcon" format="reference" />
        <attr name="leftBtnClick" format="string" />

        <attr name="rightBtn1Text" format="string" />
        <attr name="rightBtn1TextColor" format="color" />
        <attr name="rightBtn1TextSize" format="dimension" />
        <attr name="rightBtn1Icon" format="reference" />
        <attr name="rightBtn1Click" format="string" />

        <attr name="rightBtn2Text" format="string" />
        <attr name="rightBtn2TextColor" format="color" />
        <attr name="rightBtn2TextSize" format="dimension" />
        <attr name="rightBtn2Icon" format="reference" />
        <attr name="rightBtn2Click" format="string" />

        <attr name="showDividerLine" format="boolean" />
    </declare-styleable>
</resources>